{
    "version": 2,
    "builds": [
      {
        "src": "src/App.jsx",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|json))$", "dest": "/build/$1" },
      { "src": "/(.*)", "dest": "/src/index.html" }
    ]
  }
  