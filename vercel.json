{
    "version": 2,
    "builds": [
        {
        "src": "package.json",
        "use": "@vercel/static-build"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
          "source": "/about",
          "destination": "/src/About.jsx"
        },
        {
            "source": "/contact",
            "destination": "/src/Contact.jsx"
        }
    ]
}